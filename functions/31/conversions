#include <iostream>
#include<string>
using namespace std;

void ooo_message(string date){
  cout<<"Thank you for your email.\n";
  cout<<"I am currently out of the office and will be back by "<<date<< ".\n";
  cout<<"I will respond as soon as I can upon my return.\n";
}
int main() {
  ooo_message("19 January");
}
